{
  "cli": {
    "version": ">= 16.9.0",
    "appVersionSource": "remote",
    "appRoot": "src/frontend"
  },
  "build": {
    "local-dev": {
      "developmentClient": false,
      "distribution": "internal",
      "credentialsSource": "remote",
      "ios": {
        "enterpriseProvisioning": "adhoc"
      },
      "env": {
        "EXPO_PUBLIC_UNIFIED_CANISTER_ID": "77fv5-oiaaa-aaaal-qsoea-cai",
        "EXPO_PUBLIC_IC_HOST": "https://ic0.app",
        "EXPO_PUBLIC_FRONTEND_CANISTER_ID": "7yetj-dqaaa-aaaal-qsoeq-cai",
        "EXPO_PUBLIC_DFX_NETWORK": "ic",
        "EXPO_PUBLIC_INTERNET_IDENTITY_URL": "https://id.ai",
        "EXPO_PUBLIC_II_INTEGRATION_CANISTER_ID": "77fv5-oiaaa-aaaal-qsoea-cai",
        "EXPO_PUBLIC_EAS_DEEP_LINK_TYPE": "custom-scheme"
      }
    },
    "local-simulator": {
      "developmentClient": false,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_UNIFIED_CANISTER_ID": "77fv5-oiaaa-aaaal-qsoea-cai",
        "EXPO_PUBLIC_IC_HOST": "https://ic0.app",
        "EXPO_PUBLIC_FRONTEND_CANISTER_ID": "7yetj-dqaaa-aaaal-qsoeq-cai",
        "EXPO_PUBLIC_DFX_NETWORK": "ic",
        "EXPO_PUBLIC_INTERNET_IDENTITY_URL": "https://id.ai",
        "EXPO_PUBLIC_II_INTEGRATION_CANISTER_ID": "77fv5-oiaaa-aaaal-qsoea-cai",
        "EXPO_PUBLIC_EAS_DEEP_LINK_TYPE": "custom-scheme"
      }
    },
    "development": {
      "developmentClient": false,
      "distribution": "internal",
      "ios": {
        "enterpriseProvisioning": "adhoc"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {}
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_PUBLIC_BUILD": "true",
        "EXPO_PUBLIC_ENABLE_PATCHES": "false",
        "EXPO_PUBLIC_ENABLE_DEV_LOGIN": "false"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
